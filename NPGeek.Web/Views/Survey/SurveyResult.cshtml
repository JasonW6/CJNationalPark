@model List<NPGeek.Web.Models.SurveyResult>
@{
    ViewBag.Title = "Survey Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="survey-results">
    <table>
        <tr>
            <th></th>
            <th>Park</th>
            <th>Votes</th>
        </tr>

        @foreach (var result in Model)
        {
            <tr>
                <th>
                    <img class="index-park-image" src="~/Content/img/parks/@(result.ParkCode.ToLower()).jpg" />
                </th>
                <td>
                    <span>@result.ParkName</span>
                </td>
                <td>
                    <span>@result.VoteCount</span>
                </td>
            </tr>
        }

    </table>
</div>