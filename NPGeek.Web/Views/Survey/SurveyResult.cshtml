@model List<NPGeek.Web.Models.SurveyResult>
@{
    ViewBag.Title = "Survey Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table>
    <tr>
        <th></th>
        <th>Park</th>
        <th>Votes</th>
    </tr>

    @foreach (var result in Model)
    {
        <tr>
            <td>
                <img class="parkImage" src="~/Content/img/parks/@(result.ParkCode.ToLower()).jpg" />
            </td>
            <td>
                <span>@result.ParkName</span>
            </td>
            <td>
                <span>@result.VoteCount</span>
            </td>
        </tr>
    }

</table>
